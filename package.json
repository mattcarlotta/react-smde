		"node-sass": "^4.14.1",
		"normalize.css": "^8.0.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-flexbugs-fixes": "^4.2.1",
		"postcss-normalize": "^9.0.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.0.5",
		"prismjs": "^1.20.0",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dev-utils": "^10.2.1",
		"react-dom": "^16.13.1",
		"react-icons": "^3.10.0",
		"react-markdown": "^4.3.1",
