{
	"name": "react-mde",
	"version": "0.0.1",
	"description": "React Simple Markdown Editor",
	"main": "dist/index.js",
	"browser": "dist/esm/index.esm.js",
	"fallback": "dist/umd/index.umd.js",
	"style": "dist/styles/react-smde.css",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "cross-env NODE_ENV=development BABEL_ENV=development PORT=8080 webpack-dev-server --config ./demo/webpack.config.js",
		"builddemo": "rimraf livedemo && cross-env NODE_ENV=production PORT=8080 webpack --config ./demo/webpack.config.js",
		"demo": "cross-env NODE_ENV=production PORT=8080 HOST=http://localhost: node ./server.js",
		"build": "cross-env NODE_ENV=production rollup -c",
		"compile": "rimraf dist && cross-env NODE_ENV=production rollup -c",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mattcarlotta/react-smde.git"
	},
	"keywords": [
		"react",
		"component",
		"markdown",
		"editor",
		"text-editor",
		"markdown-editor"
	],
	"author": "Matt Carlotta",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mattcarlotta/react-mde.git/issues"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"npm run builddemo"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-export-default-from": "^7.8.3",
		"@babel/plugin-proposal-export-namespace-from": "^7.8.3",
		"@babel/plugin-transform-modules-commonjs": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@babel/runtime": "^7.8.4",
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"autoprefixer": "^9.7.4",
		"babel-jest": "^25.1.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^7.0.0",
		"css-loader": "^3.4.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"error-overlay-webpack-plugin": "^0.4.1",
		"express": "^4.17.1",
		"file-loader": "^5.0.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^4.0.0-beta.11",
		"husky": "^4.2.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.1.0",
		"jest-css-modules-transform": "^4.0.0",
		"jsdom": "^16.1.0",
		"lint-staged": "^10.0.7",
		"mini-css-extract-plugin": "^0.9.0",
		"morgan": "^1.9.1",
		"node-sass": "^4.13.1",
		"normalize.css": "^8.0.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.19.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dev-utils": "^10.1.0",
		"react-dom": "^16.12.0",
		"react-markdown": "^4.3.1",
		"rimraf": "^3.0.1",
		"rollup": "^1.31.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-eslint": "^7.0.0",
		"rollup-plugin-filesize": "^6.2.1",
		"rollup-plugin-postcss": "^2.0.6",
		"rollup-plugin-terser": "^5.2.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"terser-webpack-plugin": "^2.3.4",
		"webpack": "^4.41.5",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.2",
		"webpack-manifest-plugin": "^2.2.0",
		"webpackbar": "^4.0.0"
	},
	"homepage": "https://github.com/mattcarlotta/react-mde.git#readme",
	"peerDependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"dependencies": {
		"rc-trigger": "^4.0.0-rc.5"
	}
}
