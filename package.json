		"webpack-manifest-plugin": "^2.2.0",
		"webpackbar": "^4.0.0"
	},
	"homepage": "https://mattcarlotta.github.io/react-smde",
	"peerDependencies": {
		"react": "^15.0.0 || ^16.0.0",
		"react-dom": "^15.0.0 || ^16.0.0"
	},
	"dependencies": {
		"@material-ui/core": "^4.10.1",
		"styled-components": "^5.1.1"
	}
}
